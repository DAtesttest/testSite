var attachTo = document.getElementsByClassName("fillTarget"),
    targetFill = document.createElement("img"),
    optionTarget = document.getElementsByClassName("optionTarget"),
    sem1 = ["cat", "dog", "red", "green"],
    sem2 = ["hat", "vat", "red", "green"],
    bigArray = [sem1, sem2];

sem1.name = "sem1";

function test(whatId) {
    for(i = 0; i < bigArray.length; i ++) {
        if (whatId == bigArray[i].name) {
            console.log(bigArray[i]);
        }
    }
}

$(optionTarget).on("click", function() {
    whatId = this.id;
    console.log(whatId);
    test(whatId);
    //chooseImages(whatId);
    return false;
});

